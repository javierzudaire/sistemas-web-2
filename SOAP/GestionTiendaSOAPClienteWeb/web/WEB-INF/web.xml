<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GestionTienda</servlet-name>
        <servlet-class>servlets.GestionTienda</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CrearTienda</servlet-name>
        <servlet-class>servlets.CrearTienda</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddProducto</servlet-name>
        <servlet-class>servlets.AddProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetProductos</servlet-name>
        <servlet-class>servlets.GetProductos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExportTienda</servlet-name>
        <servlet-class>servlets.ExportTienda</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImportTienda</servlet-name>
        <servlet-class>servlets.ImportTienda</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ImportProducto</servlet-name>
        <servlet-class>servlets.ImportProducto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExportProducto1</servlet-name>
        <servlet-class>servlets.ExportProducto1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ExportProducto2</servlet-name>
        <servlet-class>servlets.ExportProducto2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GestionTienda</servlet-name>
        <url-pattern>/GestionTienda</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CrearTienda</servlet-name>
        <url-pattern>/CrearTienda</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddProducto</servlet-name>
        <url-pattern>/AddProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetProductos</servlet-name>
        <url-pattern>/GetProductos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExportTienda</servlet-name>
        <url-pattern>/ExportTienda</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImportTienda</servlet-name>
        <url-pattern>/ImportTienda</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ImportProducto</servlet-name>
        <url-pattern>/ImportProducto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExportProducto1</servlet-name>
        <url-pattern>/ExportProducto1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ExportProducto2</servlet-name>
        <url-pattern>/ExportProducto2</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
